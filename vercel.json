{
  "version": 2,
  "builds": [
    {
      "src": "web-interface/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/web-interface/server.js"
    },
    {
      "src": "/upload-outfit",
      "dest": "/web-interface/server.js"
    },
    {
      "src": "/photos/(.*)",
      "dest": "/web-interface/server.js"
    },
    {
      "src": "/health",
      "dest": "/web-interface/server.js"
    },
    {
      "src": "/chat",
      "dest": "/web-interface/server.js"
    },
    {
      "src": "/get-recommendations",
      "dest": "/web-interface/server.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/web-interface/manifest.json"
    },
    {
      "src": "/sw.js",
      "dest": "/web-interface/sw.js"
    },
    {
      "src": "/icon-192.png",
      "dest": "/web-interface/icon-192.png"
    },
    {
      "src": "/icon-512.png",
      "dest": "/web-interface/icon-512.png"
    },
    {
      "src": "/Stylisti.png",
      "dest": "/web-interface/Stylisti.png"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|ico|svg|css|js))",
      "dest": "/web-interface/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web-interface/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}